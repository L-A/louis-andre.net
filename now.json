{
	"version": 2,
	"name": "net",
	"alias": ["2019.louis-andre.net"],
	"builds": [{ "src": "next.config.js", "use": "@now/next" }],
	"routes": [
		{
			"src": "/_next/static/(?:[^/]+/pages)/.+",
			"headers": { "cache-control": "immutable" }
		},
		{
			"src": "^/(favicon.ico|manifest.json|humans.txt|robots.txt)$",
			"dest": "/static/$1",
			"headers": { "cache-control": "max-age=300 must-revalidate" }
		}
	]
}
